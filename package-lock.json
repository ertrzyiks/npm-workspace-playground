{
  "name": "npm-workspace-playground",
  "lockfileVersion": 2,
  "requires": true,
  "packages": {
    "": {
      "workspaces": [
        "apps/*",
        "libs/*",
        "tools/*"
      ]
    },
    "apps/demo": {
      "name": "@playground/demo",
      "version": "1.0.0",
      "license": "MIT",
      "dependencies": {
        "@playground/hello": "1.0.0",
        "lodash.pick": "^4.4.0"
      }
    },
    "libs/hello": {
      "name": "@playground/hello",
      "version": "1.0.0",
      "license": "MIT"
    },
    "node_modules/@playground/demo": {
      "resolved": "apps/demo",
      "link": true
    },
    "node_modules/@playground/hello": {
      "resolved": "libs/hello",
      "link": true
    },
    "node_modules/@playground/print": {
      "resolved": "tools/print",
      "link": true
    },
    "node_modules/lodash.pick": {
      "version": "4.4.0",
      "resolved": "https://registry.npmjs.org/lodash.pick/-/lodash.pick-4.4.0.tgz",
      "integrity": "sha1-UvBWEP/53tQiYRRB7R/BI6AwAbM="
    },
    "tools/print": {
      "name": "@playground/print",
      "version": "1.0.0",
      "license": "MIT"
    }
  },
  "dependencies": {
    "@playground/demo": {
      "version": "file:apps/demo",
      "requires": {
        "@playground/hello": "1.0.0",
        "lodash.pick": "^4.4.0"
      }
    },
    "@playground/hello": {
      "version": "file:libs/hello"
    },
    "@playground/print": {
      "version": "file:tools/print"
    },
    "lodash.pick": {
      "version": "4.4.0",
      "resolved": "https://registry.npmjs.org/lodash.pick/-/lodash.pick-4.4.0.tgz",
      "integrity": "sha1-UvBWEP/53tQiYRRB7R/BI6AwAbM="
    }
  }
}
